<script>
    $(document).ready(function () {
        $.ajax({
            type: "POST",
            url: '@Url.Action("BindTable", "Product")',
            dataType: "json",
            success: function (data) {
                debugger
                for (var i = 0; i < data.length; i++) {
                    debugger;
                    var SrNo = i + 1;
                    $("#productTable").append(
                        '<tr><td>' + SrNo + '</td><td>' + data[i].productName + '</td><td>' + data[i].productCompany + '</td><td>' + data[i].packing + '</td><td>' + data[i].HSNSACCode + '</td><td>' + data[i].manufacturerName + '</td><td style="display:none">' + data[i].productNameID + '</td></tr>');
                };
                $('#productTable').DataTable({
                    "fixedHeader": true,
                    "paging": true,
                    "lengthChange": true,
                    "searching": true,
                    "ordering": true,
                    "info": true,
                    "autoWidth": true,
                    "responsive": false,
                });
                var buttons = new $.fn.dataTable.Buttons($('#productTable').DataTable(), {
                    buttons: [
                        {
                            extend: 'excel',
                            text: '<li class="fa fa-file-excel-o fa-lg" style="font-weight: 600;"></li><br>Excel'
                        }
                    ]
                }).container().appendTo($('#productTableExcelExport'));
            },
            error: function (result) {
                alert("Error grd bind");
            }
        });
    });
</script>
<script>
    $(document).ready(function () {
        $('#productTable').delegate('td', 'click', function () {
            $("#save").html("<i class='fa fa-save'></i>Update");
            debugger;
            var row_num = parseInt($(this).parent().index());
            var productNameID = parseInt(document.getElementById("productTable").tBodies[0].rows[row_num].cells[6].innerHTML);

            $.ajax({
                type: "POST",
                url: '@Url.Action("Rebind_Data", "Product")',
                data: { id: productNameID },
                dataType: "json",
                success: function (data) {
                    debugger;

                    $("#productNameID").val(data[0].productNameID);
                    $("#productName").val(data[0].productName);
                    $("#productCompanyID").val(data[0].productCompanyID);
                    $("#productCompany").val(data[0].productCompany);
                    $("#packingID").val(data[0].packingID);
                    $("#packing").val(data[0].packing);
                    $("#contains").val(data[0].contains);
                    $("#containsQuantity").val(data[0].containsQuantity);
                    $("#sellLoose").val(data[0].sellLoose);
                    $("#genericID").val(data[0].genericID);
                    $("#generic").val(data[0].generic);
                    $("#HSNSACCode").val(data[0].HSNSACCode);
                    $("#SKUCode").val(data[0].SKUCode);
                    $("#chapterCode").val(data[0].chapterCode);
                    $("#manufacturerName").val(data[0].manufacturerName);
                    $("#maximumLevel").val(data[0].maximumLevel);
                    $("#minimumLevel").val(data[0].minimumLevel);
                    $("#scheduled").val(data[0].scheduled);
                    $("#categoryID").val(data[0].categoryID);
                    $("#category").val(data[0].category);
                    $("#barCode").val(data[0].barCode);
                    $("#discontinueDate").val(data[0].discontinueDate);
                    $("#extrafield").val(data[0].extrafield);
                    $("#detailextrafield").val(data[0].detailextrafield);
                    $("#gst").val(data[0].gst);
                    $("#sgst").val(data[0].sgst);
                    $("#cgst").val(data[0].cgst);
                    $("#utgst").val(data[0].utgst);
                    $("#cess").val(data[0].cess);

                    if (data[0].sellLoose == "True")
                    {
                        $("#sellLoose").prop("checked",true);
                    }
                    else
                    {
                        $("#sellLoose").prop("checked", false);
                    }

                    if (data[0].scheduled == "True") {
                        $("#scheduled").prop("checked", true);
                    }
                    else {
                        $("#scheduled").prop("checked", false);
                    }

                    if (data[0].detailextrafield == "True") {
                        $("#detailextrafield").prop("checked", true);
                    }
                    else {
                        $("#detailextrafield").prop("checked", false);
                    }

                },
            });

    });
    });
</script>
<script>
   
    function Delete(productNameID) {
            debugger
            if (productNameID != "")
            {
                var ans = confirm("Are you sure you want to delete this record ?");
                if (ans) {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("DeleteProduct", "Product")',
                        data: { productNameID, productNameID },
                        datatype: "json",
                        success: function (data) {
                            if (data == "Done") {
                                alert("Product Deleted Successfully");
                                location.reload(true);
                                Restore();
                            }
                            else {
                                alert(data);
                            }
                        },
                        error: function (errormessage) {
                            alert(errormessage.responseText);
                        }
                    })
                }
            }
        }
   
</script>
<script>
    $(function () {
        $("#HSNSACCode").change(function () {
            debugger
            var code = $("#HSNSACCode").val();
            $.ajax({
                type: "POST",
                url: '@Url.Action("Get_HSN_GST", "Product")',
                data: '{HSNSACCode: ' + JSON.stringify(code) + '}',
                contentType: "application/json; charset=utf-8",
                success: function (searchList) {
                    debugger;
                    $("#gst").val(searchList[0].gst);
                    $("#utgst").val($("#gst").val());
                    var percntg = parseInt($("#gst").val());
                    if (percntg != 0 || percntg != null) {
                        $("#cgst").val(percntg / 2);
                        $("#sgst").val(percntg / 2);
                    }
                },
            });
        });
    });
</script>

